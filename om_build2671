create table account (account_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, city varchar(100) not null, client_id int4 not null unique, country varchar(100) not null, failed_login_count_b4_user_lock int4, license_key varchar(100) not null, license varchar(100) not null, logo bytea, is_master boolean default false, account_name varchar(50) not null unique, phone_number varchar(100), postal_code varchar(100), state varchar(100) not null, street varchar(255) not null, oauth2_server_configuration_id int8, saml_idp_metadata_id int8, primary key (account_id), unique (account_name));
create table announcement (annonouncement_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, expiration_time timestamp not null, annonouncement_message varchar(128) not null, object_name varchar(128), object_id int8, application_id int8 not null, user_id int8 not null, primary key (annonouncement_id));
create table application (application_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, is_allow_rooted_device_access boolean default false, is_allow_save_credentials_on_device boolean default false, app_binary_data int8, is_app_store_app boolean default false, authentication_provider varchar(255) not null, body_color varchar(50) default '#FFFFFF', body_header_color varchar(50) default '#117BB4', body_subheader_color varchar(50) default '#EF7A10', body_text_color varchar(50) default '#333333', bundle_identifier varchar(255), bundle_version varchar(255), description text, disable_crash_log_upload boolean default false, is_external boolean default false, sha1_hash varchar(255), icon_binary_data int8, identity_service_field varchar(50) default '#FFFFFF', app_key int4 not null unique, log_level varchar(255), max_version int4, menu_color varchar(50) default '#333333', menu_text_color varchar(50) default '#FFFFFF', min_version int4, name varchar(50) not null, native_push_notification boolean default false, os varchar(255) not null, registration_mode varchar(255) not null, is_require_passcode boolean default false, secret varchar(50) not null, server_url varchar(255), session_duration int4, app_binary_size int4, app_splash_data int8, state varchar(255) not null, tos text, title_color varchar(50) default '#117BB4', title_text_color varchar(50) default '#FFFFFF', url text, version_name varchar(255), account_id int8 not null, admin_application_id int8, authenticating_service_endpoint_id int8, custom_auth_provider_id int8, identity_service_id int8, saml_idp_metadata_id int8, primary key (application_id), unique (account_id, name));
create table application_binary_data (application_binary_data_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, file_data bytea not null, file_name varchar(255) not null, parent_id int8, type varchar(255) not null, user_name varchar(255) not null, version varchar(255), application_id int8 not null, primary key (application_binary_data_id));
create table application_category (application_category_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, application_id int8 not null, category_id int8 not null, primary key (application_category_id), unique (application_id, category_id));
create table application_design (application_design_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, template_name varchar(50), navigation_type int4 not null, application_id int8 not null, landing_screen_id int8, theme_id int8 not null, primary key (application_design_id));
create table application_fact (application_fact_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, bytes_in int8, bytes_out int8, client_object_creates int4, client_object_deletes int4, devices int4, downloads int4, launches int4, client_object_list int4, logins int4, registrations int4, client_object_retrieve int4, client_object_updates int4, users int4, account_id int8 not null, application_id int8 not null, time_by_day_id int8 not null, primary key (application_fact_id), unique (application_id, time_by_day_id));
create table application_media (application_media_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, file_data bytea not null, file_name varchar(255) not null, media_text text not null, application_id int8 not null, primary key (application_media_id), unique (application_id, file_name));
create table application_notification_template (application_notification_template_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, application_id int8 not null, notification_template_id int8 not null, role_id int8, primary key (application_notification_template_id), unique (application_id, notification_template_id));
create table application_role (application_role_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, application_id int8 not null, role_id int8 not null, primary key (application_role_id), unique (application_id, role_id));
create table application_service (application_service_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, is_create_enabled boolean default true, is_delete_enabled boolean default false, enable_offline_write boolean default false, enable_optimistic_locking boolean default false, is_update_enabled boolean default false, application_id int8 not null, application_role_id int8, service_id int8 not null, primary key (application_service_id), unique (application_role_id, service_id));
create table application_service_block (application_service_block_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, application_id int8 not null, application_role_id int8, service_block_id int8 not null, primary key (application_service_block_id), unique (application_role_id, service_block_id));
create table application_webhook_filter (application_webhook_filter_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, data_format_type varchar(255), is_encrypted boolean default false, filter text, application_id int8 not null, service_id int8 not null, primary key (application_webhook_filter_id));
create table batch_service (batch_service_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, code text not null, cron_expression varchar(128) not null, email_flag boolean default false, language varchar(25) default 'JS' not null, name varchar(128), notes text, parent_version_number int4, running_flag boolean default false, start_time timestamp not null, timezone varchar(25) default 'UTC', to_users varchar(255), version_description varchar(25), version_number int4, account_id int8 not null, notification_server_configuration_id int8, primary key (batch_service_id), unique (account_id, name, version_number));
create table batch_service_inactive (batch_service_inactive_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, end_time timestamp not null, start_time timestamp not null, batch_service_id int8 not null, primary key (batch_service_inactive_id));
create table batch_service_log (batch_service_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, is_completed boolean default false, end_time timestamp, host varchar(50) not null, message text, name varchar(50) not null, is_skipped boolean default false, start_time timestamp not null, is_success boolean default false, account_id int8 not null, batch_service_id int8 not null, primary key (batch_service_log_id));
create table category (category_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, description text, name varchar(50) not null unique, primary key (category_id));
create table checkin (checkin_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, lattitude float8, longitude float8, checkin_object varchar(255), object_id int8, checkin_tag varchar(255), application_id int8 not null, device_id int8 not null, primary key (checkin_id), unique (application_id, device_id, checkin_object, object_id), unique (application_id, device_id, checkin_tag));
create table custom_auth_provider (custom_auth_provider_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, cache_duration int4, code text not null, language varchar(25) default 'JS' not null, name varchar(128), notes text, saml_flow_passthru boolean default false, update_date timestamp, account_id int8 not null, primary key (custom_auth_provider_id), unique (account_id, name));
create table custom_service_object_field (service_object_field_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, name varchar(50) not null, required boolean default false, value_type varchar(25) not null, updateable boolean default false, service_id int8 not null, primary key (service_object_field_id));
create table datasource_fact (datasource_fact_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, avg_hit_time float8, failure_count int4, max_hit_time int8, method varchar(255) not null, min_hit_time int8, service_endpoint_name varchar(255) not null, success_count int4, total_hits int4, url varchar(255) not null, account_id int8 not null, time_by_day_id int8 not null, primary key (datasource_fact_id), unique (service_endpoint_name, method, url, time_by_day_id));
create table datasource_monitoring_log (datasource_monitoring_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, is_completed boolean default false, end_time timestamp, host varchar(50) not null, message text, name varchar(50) not null, is_skipped boolean default false, start_time timestamp not null, is_success boolean default false, account_id int8 not null, datasource_monitoring_service_id int8 not null, primary key (datasource_monitoring_log_id));
create table datasource_monitoring_service (datasource_monitoring_service_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, cron_expression varchar(128) not null, email_flag boolean default false, name varchar(128), notes text, running_flag boolean default false, start_time timestamp not null, timezone varchar(25) default 'UTC', to_users varchar(255), account_id int8 not null, notification_server_configuration_id int8, service_endpoint_id int8 not null, primary key (datasource_monitoring_service_id), unique (service_endpoint_id), unique (account_id, name));
create table default_notification_template (default_notification_template_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, delay int4, from_address varchar(128), name varchar(50) not null, notes text, subject varchar(128), notification_type varchar(25) not null, body text not null, notification_trigger varchar(25) not null, primary key (default_notification_template_id), unique (name));
create table device (device_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, device_uid varchar(255) not null, device_key varchar(255) not null, last_login TIMESTAMP, device_model varchar(255), remote_wipe_acknowledge_date TIMESTAMP, remote_wiped boolean default false not null, account_id int8 not null, application_id int8, platform_id int8 not null, user_id int8 not null, primary key (device_id), unique (user_id, device_uid));
create table device_application (device_application_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, app_version varchar(255), badge_count int4, is_deleted boolean default false not null, download_end_date TIMESTAMP, download_start_date TIMESTAMP, last_verification TIMESTAMP, ns_device_id varchar(255), remote_wipe_acknowledge_date TIMESTAMP, remote_wiped boolean default false not null, application_id int8 not null, device_id int8 not null, primary key (device_application_id), unique (device_id, application_id));
create table device_event_log (device_event_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, action varchar(255), app_name varchar(255), category varchar(255), device_uid varchar(255), label varchar(255), log_level varchar(255) not null, session_token varchar(255), event_timestamp timestamp not null, user_name varchar(255), value varchar(255), app_version varchar(255), account_id int8, application_id int8, device_id int8, user_id int8, primary key (device_event_log_id));
create table device_log (device_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, file_data bytea not null, file_name varchar(255) not null, source_ip_address varchar(50), account_id int8 not null, application_id int8 not null, device_id int8 not null, user_id int8 not null, primary key (device_log_id));
create table field (field_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, font_color varchar(50) not null, font_name varchar(50) not null, font_size int4 not null, font_type varchar(255) not null, alignment_type varchar(50) default 'CENTER' not null, color varchar(50), height int4 not null, image bytea, label varchar(50), link varchar(255), top_left_x int4 not null, top_left_y int4 not null, type varchar(255) not null, width int4 not null, next_screen_id int8, screen_row_id int8 not null, service_object_field_id int8, primary key (field_id), unique (screen_row_id, service_object_field_id));
create table function_parameter (function_parameter_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, definition varchar(50) not null, name varchar(50) not null, param_type varchar(25), required boolean default false, value_type varchar(25) not null, function_id int8 not null, primary key (function_parameter_id), unique (function_id, definition));
