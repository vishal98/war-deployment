create table poll (poll_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, end_time timestamp, object_name varchar(128), object_id int8, start_time timestamp not null, title varchar(128) not null, type varchar(255) not null, account_id int8 not null, user_id int8 not null, primary key (poll_id), unique (account_id, title));
create table poll_choice (poll_choice_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, image bytea, text varchar(255), poll_id int8 not null, primary key (poll_choice_id));
create table poll_response (poll_response_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, selection text, poll_id int8 not null, poll_choice_id int8 not null, user_id int8 not null, primary key (poll_response_id), unique (poll_id, user_id, poll_choice_id));
create table request_entry (request_entry_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, app_name varchar(255), bytes_in int4, bytes_out int4, client_method varchar(255), code int4, device_uid varchar(255), elapsedNanos int8, error varchar(4095), headers varchar(4095), method varchar(4095), parameters varchar(4095), request_id int8, session_token varchar(255), source_ip_address varchar(50), start_time timestamp not null, status int4, url varchar(4095), user_name varchar(255), app_version varchar(50), account_id int8, application_id int8, device_id int8, user_id int8, primary key (request_entry_id));
create table request_id_dispenser (request_id_dispenser_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, blocksize int4, next int8, primary key (request_id_dispenser_id));
create table saml_idp_metadata (saml_idp_metadata_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, name varchar(255) not null, data text not null, login_url text, account_id int8 not null, primary key (saml_idp_metadata_id), unique (account_id, name));
create table screen (screen_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, font_color varchar(50) not null, font_name varchar(50) not null, font_size int4 not null, font_type varchar(255) not null, color varchar(50), content text, load_filter text, order_directive varchar(50), order_field varchar(50), template_name varchar(50), title varchar(50) not null, type varchar(255) not null, url text, widget_id varchar(255), application_design_id int8 not null, service_id int8, primary key (screen_id), unique (application_design_id, title, type));
create table screen_row (screen_row_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, color varchar(50), height int4 not null, name varchar(50) not null, screen_row_order int4 not null, top_left_x int4 not null, top_left_y int4 not null, width int4 not null, next_screen_id int8, screen_id int8 not null, primary key (screen_row_id), unique (screen_id, screen_row_order), unique (screen_id, name));
create table service (service_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, cache_duration int4, name varchar(128), notes text, update_date timestamp, account_id int8 not null, key_service_object_id int8, primary key (service_id), unique (account_id, name));
create table service_block (service_block_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, cache_duration int4, code text not null, language varchar(25) default 'JS' not null, name varchar(128), notes text, update_date timestamp, account_id int8 not null, primary key (service_block_id), unique (account_id, name));
create table service_block_parameter (service_block_parameter_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, name varchar(50) not null, required boolean default false, value_type varchar(25) not null, service_block_id int8 not null, primary key (service_block_parameter_id), unique (service_block_id, name));
create table service_endpoint (service_endpoint_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, authentication_type varchar(25) not null, connection_expiration int8, max_connections int4, name varchar(255) not null, notes text, type varchar(25) not null, account_id int8 not null, primary key (service_endpoint_id), unique (account_id, name));
create table service_endpoint_field (service_endpoint_field_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, file_data bytea, name varchar(255) not null, value_type varchar(255) not null, value varchar(520), service_endpoint_id int8 not null, primary key (service_endpoint_field_id), unique (service_endpoint_id, name));
create table service_endpoint_operation (service_endpoint_operation_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, operation varchar(255) not null, operation_type varchar(25) not null, service_endpoint_id int8 not null, primary key (service_endpoint_operation_id));
create table service_function (service_function_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, cache_duration int4, content_type varchar(25), function_name varchar(128) not null, method_name varchar(255), name varchar(255), notes text, update_date timestamp, account_id int8 not null, service_endpoint_id int8 not null, primary key (service_function_id), unique (account_id, name));
create table service_object_field (service_object_field_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, definition varchar(255) not null, name varchar(50) not null, required boolean default false, value_type varchar(25) not null, updateable boolean default false, service_service_endpoint_id int8 not null, primary key (service_object_field_id), unique (service_service_endpoint_id, definition), unique (service_service_endpoint_id, name));
create table service_object_notification_template (service_object_notification_template_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, delay int4, from_address varchar(128), name varchar(50) not null, notes text, subject varchar(128), notification_type varchar(25) not null, attachment_field varchar(255), attachment_name varchar(255), badge_count varchar(255), body text, service_event varchar(25) not null, loggedin_devices_only boolean default false, recipient_rule varchar(255), is_text boolean default false, application_id int8, notification_server_configuration_id int8, role_id int8, service_id int8 not null, primary key (service_object_notification_template_id), unique (service_id, name), unique (service_id, notification_type, service_event, role_id));
create table service_provider_log (service_provider_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, account_id int8 not null, bytes_in int4, bytes_out int4, elapsedNanos int8, error varchar(4095), headers varchar(4095), method varchar(4095), parameters varchar(4095), post_param varchar(255), response varchar(255), service_endpoint_name varchar(4095), start_time timestamp not null, status int4, url varchar(4095), user_name varchar(255), primary key (service_provider_log_id));
create table service_service_endpoint (service_service_endpoint_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, connection_expiration int8, max_connections int4, object_name varchar(255) not null, optional boolean default false, is_primary_object boolean default true, join_service_object_id int8, service_id int8 not null, service_endpoint_id int8 not null, primary key (service_service_endpoint_id), unique (service_id, service_endpoint_id, object_name));
create table super_user (super_user_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, account_id int8, user_id int8 not null, primary key (super_user_id), unique (user_id));
create table theme (theme_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, font_color varchar(50) not null, font_name varchar(50) not null, font_size int4 not null, font_type varchar(255) not null, background_color varchar(50), background_image bytea, footer_color varchar(255), footer_font_color varchar(255), footer_font_name varchar(255), footer_font_size int4, footer_font_type int4, footer_image bytea, footer_title varchar(255), header_color varchar(50), header_font_color varchar(50), header_font_name varchar(50), header_font_size int4, header_font_type int4, header_image bytea, header_title varchar(50), name varchar(50) not null unique, primary key (theme_id));
create table time_by_day_epoch (time_by_day_epoch_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, the_date date not null, day_of_month int4 not null, day_of_year int4 not null, epoch int4 not null, month_of_year int4 not null, quarter int4 not null, week_of_year int4 not null, the_year int4 not null, primary key (time_by_day_epoch_id));
create table user_application_role (user_application_role_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, accepted_tos boolean default false, approved boolean default false, comment varchar(255), log_level varchar(255), pin varchar(40), rating int4, application_id int8 not null, application_role_id int8, user_id int8 not null, primary key (user_application_role_id), unique (user_id, application_role_id));
create table user_fact (user_fact_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, apps int4, bytes_in int8, bytes_out int8, client_object_creates int4, client_object_deletes int4, devices int4, client_object_list int4, logins int4, notifications int4, client_object_retrieve int4, client_object_updates int4, account_id int8 not null, time_by_day_id int8 not null, user_id int8 not null, primary key (user_fact_id), unique (user_id, time_by_day_id));
create table user_favorite_object (user_favorite_object_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, object_id varchar(255) not null unique, device_id int8 not null, service_id int8 not null, user_id int8 not null, primary key (user_favorite_object_id), unique (user_id, device_id, service_id, object_id));
create table user_shared_object (user_shared_object_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, object_id varchar(255) not null unique, device_id int8 not null, service_id int8 not null, shared_user_id int8 not null, user_id int8 not null, primary key (user_shared_object_id), unique (user_id, shared_user_id, device_id, service_id, object_id));
