create table halomem_user (halomem_user_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, admin boolean default false, is_anonymous boolean default false, display_name varchar(255), ldap_domain varchar(128), ldap_domain_id varchar(255), email varchar(128), employee_id varchar(128), failed_login_count_b4_lock int4, first varchar(128), last varchar(128), locked boolean default false, password varchar(128), phone_number varchar(128), remote_wiped boolean default false, sam_account_name varchar(255), source varchar(40), token varchar(40), account_id int8 not null, primary key (halomem_user_id), unique (account_id, email));
create table java_provider (java_provider_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, provider_binary bytea not null, service_identifier varchar(255), provider_scope varchar(255), symbolic_name varchar(128) not null, service_endpoint_id int8 not null, primary key (java_provider_id), unique (service_endpoint_id));
create table ldap_server_configuration (ldap_server_configuration_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, name varchar(255) not null, domain_id varchar(128), email_field varchar(128) default 'mail', home_directory_field varchar(255), ldap_id_field varchar(128) default 'objectSid' not null, page_size int4, admin_password varchar(255), port int4, server varchar(255) not null, base_dn varchar(255), admin_username varchar(255) not null, account_id int8 not null, primary key (ldap_server_configuration_id), unique (account_id, server, port, base_dn), unique (account_id, name));
create table navigation_item (navigation_item_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, font_color varchar(50) not null, font_name varchar(50) not null, font_size int4 not null, font_type varchar(255) not null, color varchar(50), height int4 not null, icon_color varchar(50), image bytea, image_font varchar(50), navigation_item_order int4 not null, title varchar(50), width int4 not null, application_design_id int8 not null, screen_id int8 not null, primary key (navigation_item_id), unique (application_design_id, navigation_item_order));
create table notification_log (notification_log_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, app_name varchar(255), device_uid varchar(255), from_user_name varchar(255), message text, ns_device_id varchar(255), notification_template_id int8, service_name varchar(255), is_success boolean default false, to_user_name varchar(255) not null, notification_trigger varchar(25) not null, notification_type varchar(25) not null, account_id int8 not null, application_id int8, from_user_id int8, to_user_id int8, primary key (notification_log_id));
create table notification_server_configuration (notification_server_configuration_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, name varchar(255) not null, notification_type varchar(25) not null, account_id int8 not null, application_id int8, primary key (notification_server_configuration_id), unique (account_id, name, notification_type), unique (account_id, name));
