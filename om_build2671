create table notification_template (notification_template_id  bigserial not null, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, active boolean default false, modified TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, delay int4, from_address varchar(128), name varchar(50) not null, notes text, subject varchar(128), notification_type varchar(25) not null, body text not null, notification_trigger varchar(25) not null, account_id int8 not null, notification_server_configuration_id int8, primary key (notification_template_id), unique (account_id, name));
